
@{
    ViewBag.Title = "ChangePassword";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<script type="text/javascript">
    $(document).ready(function () {
        $("#mainHome").addClass("active open");
        $("#childChangePassword").addClass("active");
      //  $("#mainHome").removeClass("active open");
        $("#childIndex").removeClass("active");
    });
</script>

<!-- Validation -->
<script src="~/App_Theme/plugins/validate/jquery.validate.min.js"></script>

<!-- Demo Scripts -->
<script src="~/App_Theme/assets/js/demo/form_validation.js"></script>

<div id="main-header" class="page-header">
    <ul class="breadcrumb">
        <li>
            <i class="icon-accessibility"></i><a href="@Url.Action("Index", "Admin")">Administrator</a>
            <span class="divider">&raquo;</span>
        </li>
        <li>
            Change Password
        </li>
    </ul>
</div>
<div class="page-content">
    <div class="row-fluid">
        <div class="span12 widget">
            <div class="widget-header">
                <span class="title"><i class="icon-tag"></i>Change Password</span>
            </div>
            <div class="widget-content form-container">
                <form class="form-horizontal" id="validate-1">
                    @*<fieldset>*@
                    <div class="control-group">
                        <div class="span3">
                        </div>
                        <div class="span6">
                            <label class="control-label" for="txtOldPasssword"> Old Passsword <span class="required">★</span></label>
                            <div class="controls">
                                <input type="password" id="txtOldPasssword" name="pass" class="span10">
                            </div>
                        </div>
                        <div class="span3">
                        </div>
                    </div>

                    <div class="control-group">
                        <div class="span3">
                        </div>
                        <div class="span6">
                            <label class="control-label" for="txtPasssword">New Passsword <span class="required">★</span></label>
                            <div class="controls">
                                <input type="password" id="txtPasssword" name="pass1" class="span10">
                            </div>
                        </div>
                        <div class="span3">
                        </div>
                    </div>

                    <div class="control-group">
                        <div class="span3">
                        </div>
                        <div class="span6">
                            <label class="control-label" for="txtConPassword">Confirm Password <span class="required">★</span></label>
                            <div class="controls">
                                <input type="password" id="txtConPassword" name="cpass1" class="span10">
                            </div>
                        </div>
                        <div class="span3">
                        </div>
                    </div>

                    @*</fieldset>*@
                    <br />
                    <div class="form-actions" style="padding-left: 330px;">
                        <input type="button" class="btn btn-primary" value="Save" id="btnNewSave" style="width:85px;" />
                        <input type="button" class="btn btn-info" value="Refresh" id="btnNewRefresh" style="width:85px;" />
                        <input type="button" class="btn btn-danger" value="Cancel" id="btnCancel" style="width:85px;" />
                       
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
@section scripts
{
    <script type="text/javascript">
        var base = '@Url.Content("~/")';
        $.getScript('@Url.Content("~/App_Js/Admin/AddUser.js")',
            function (data, textStatus, jqxhr) {
                console.log('Script File Load Status=' + jqxhr.status);
            });
    </script>
}