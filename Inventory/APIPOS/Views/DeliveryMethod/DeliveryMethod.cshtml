
@{
    ViewBag.Title = "DeliveryMethod";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style type="text/css">
    #tbl tr:nth-child(even) {
        background-color: #f2f2f2 !important;
    }

    #tbl td, #tbl th {
        border: 1px solid #ddd !important;
    }

    #tbl th {
        padding-top: 12px;
        padding-bottom: 12px;
        background-color: #f2f2f2;
    }
</style>

<script type="text/javascript">
    $(document).ready(function () {
        $("#mainGeneral").addClass("active open");
        $("#childAddressType").addClass("active");

    });
</script>
<div id="main-header" class="page-header">
    <ul class="breadcrumb">
        <li>
            <i class="icon-accessibility"></i><a href="@Url.Action("Index", "Admin")">Administrator</a>
            <span class="divider">&raquo;</span>
        </li>
        <li>
            Delivery Method
            @*<a href="@Url.Action("DeliveryMethod", "DeliveryMethod")"></a>*@
        </li>
    </ul>
</div>
<div class="page-content">
    <div class="row-fluid">
        <div class="span12 widget">
            <div class="widget-header">
                <span class="title"><i class="icon-tag"></i> Delivery Method</span>
            </div>
            <div class="widget-content form-container">
                <form class="form-horizontal">
                    <fieldset>
                        <div class="control-group">
                            <label class="control-label" for="txtName">Delivery Method <span class="required">*</span></label>
                            <div class="controls">
                                <input type="text" id="txtName" class="span10">
                                <input type="hidden" id="ID" value="0" />
                            </div>
                        </div>
                        @*<div class="control-group">
                            <div class="controls">
                                <label class="checkbox">
                                    <input type="checkbox" id="chkIsActive" value="option1" class="uniform" checked>
                                    IsActive
                                </label>
                            </div>
                        </div>*@
                    </fieldset>
                    <br />
                    @Html.Partial("_layoutPageButton")
                </form>
            </div>
        </div>
    </div>
    <div class="row-fluid">
        <div class="span12 widget">
            <div class="widget-header">
                <span class="title">
                    <i class="icol-table"></i>List
                </span>
            </div>
            <div id="divTable" class="widget-content table-container">
            </div>
        </div>
    </div>
</div>
@section scripts
{
    <script type="text/javascript">
        var base = '@Url.Content("~/")';
        $.getScript('@Url.Content("~/App_Js/Basic/DeliveryMethod.js")',
            function (data, textStatus, jqxhr) {
                console.log('Script File Load Status=' + jqxhr.status);
            });
    </script>
}