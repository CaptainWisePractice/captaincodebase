
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style type="text/css">
    #demo-dtable-04 td, #demo-dtable-04 th {
        border: 1px solid #ddd !important;
        padding: 5px;
    }

    #demo-dtable-04 tr:hover {
        background-color: #ddd !important;
    }

    #demo-dtable-04 tr:nth-child(even) {
        background-color: #f2f2f2 !important;
    }

    #demo-dtable-04 th {
        padding-top: 6px;
        padding-bottom: 6px;
        text-align: left;
        background-color: #2da3bf !important;
        color: black;
    }

    /* Modal Css*/

    /* The Modal (background) */
    .modal {
        display: none;
        position: fixed;
        z-index: 10000;
        padding-top: 0px;
        left: 0;
        top: 0;
        width: 100% !important;
        height: 100% !important;
        overflow: auto;
        background-color: rgb(0,0,0);
        background-color: rgba(0,0,0,0.4);
        margin: 0px !important;
    }

    /* Modal Content */
    .modal-content {
        position: relative;
        margin: auto;
        padding: 0;
        width: 83%;
        -webkit-animation-name: animatetop;
        -webkit-animation-duration: 0.4s;
        animation-name: animatetop;
        animation-duration: 0.4s;
        top: 10%;
    }

    /* Add Animation */


    /* The Close Button */
    .close {
        color: white;
        float: right;
        font-size: 28px;
        font-weight: bold;
    }

        .close:hover,
        .close:focus {
            color: #000;
            text-decoration: none;
            cursor: pointer;
        }

    .modal-header {
        padding: 2px 16px;
        background-color: #5cb85c;
        color: white;
    }

    .modal-body {
        padding: 2px 16px;
        min-height: 530px !important;
        overflow: hidden;
    }

    .modal-footer {
        padding: 2px 16px;
        background-color: #5cb85c;
        color: white;
    }
</style>
<!-- Uniform Stylesheet -->
<link rel="stylesheet" href="~/App_Theme/plugins/uniform/css/uniform.default.css" media="screen">

<script type="text/javascript">
    $(document).ready(function () {
        $("#mainDespatch").addClass("active open");
        $("#childCustomerPickUp").addClass("active");
        $("#mainHome").removeClass("active open");
        $("#childIndex").removeClass("active");

    });

</script>
<div id="main-header" class="page-header">
    <ul class="breadcrumb">
        <li>
            <i class="icon-truck"></i><a href="@Url.Action("Home", "DespatchHome")">Despatch</a>
            <span class="divider">&raquo;</span>
        <li>
            Warehouse PickUp
            @*<a href="@Url.Action("Home", "DespatchHome")"> Customer PickUp</a>*@
        </li>
    </ul>
</div>
<div class="page-content">
    <div class="row-fluid">
        <div class="span12 widget">
            <div class="widget-header">
                <span class="title"><i class="icon-tag"></i>Warehouse Pickup </span>
            </div>
            <div class="widget-content form-container">
                <form class="form-horizontal">
                    <fieldset>
                        <div class="span12">
                            <div class="control-group">
                                <div class="span4">
                                    <label class="control-label">From Date</label>
                                    <div class="controls">
                                        <input type="text" id="txtFrom" class="required span12 ftDate" />

                                    </div>
                                </div>
                                <div class="span4">
                                    <label class="control-label">To Date</label>
                                    <div class="controls">
                                        <input type="text" id="txtTo" class="required span12 ftDate" />
                                    </div>
                                </div>
                                <div class="span4">
                                    <label class="control-label"></label>
                                    <div class="controls">
                                        @*<input type="button" class="btn btn-info" value="Search" id="btnSearch" />*@
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="span12" style="margin-left: 0px;">
                            <div class="control-group">
                                <div class="controls" style="margin-left: 0px">
                                    <div id="divTable" class="widget-content table-container">

                                    </div>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                    <br />
                    @Html.Partial("_layoutPageButton")
                </form>
            </div>
        </div>
    </div>

    <!-- The Modal Add POD-->
    <div id="PODModal" class="modal" data-backdrop="false">

        <!-- Modal content -->
        @Html.Partial("../DespatchCenter/_PODModal")
    </div>

</div>
@section scripts
{
    <script type="text/javascript">
            var base = '@Url.Content("~/")';
            $.getScript('@Url.Content("~/App_Js/Inventory/WarehousePickup.js")',
                function (data, textStatus, jqxhr) {
                    console.log('Script File Load Status=' + jqxhr.status);
                });
    </script>
}

