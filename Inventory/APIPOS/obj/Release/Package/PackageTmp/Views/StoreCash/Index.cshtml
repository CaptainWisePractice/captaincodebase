
@{
    ViewBag.Title = "CustomerPrice";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<script type="text/javascript">
    $(document).ready(function () {
        $("#mainHome").addClass("active open");
        $("#childStoreCash").addClass("active");
        // $("#mainHome").removeClass("active open");
        $("#childIndex").removeClass("active");
    });
</script>

<div id="main-header" class="page-header">
    <ul class="breadcrumb">
        <li>
            <i class="icon-accessibility"></i><a href="@Url.Action("Index", "Admin")">Admin</a>
            <span class="divider">&raquo;</span>
        </li>
        <li>
            Store Cash Add
            @*<a href="@Url.Action("Index", "Admin")">Add User</a>*@
        </li>
    </ul>
</div>
<div class="page-content">
    <div class="row-fluid">
        <div class="span12 widget">
            <div class="widget-header">
                <span class="title"><i class="icon-tag"></i>Store Cash Add </span>
            </div>
            <div class="widget-content form-container">
                <form class="form-horizontal">
                    <fieldset>
                        <div class="control-group">
                            <div class="span2">
                                <input type="hidden" id="hfId" value="0" />
                            </div>
                            <div class="span7">
                                <label class="control-label" for="ddlHead">Outlet Name <span class="required">★</span></label>
                                <div class="controls">
                                    <select id="ddlOutlet" class="span12 select2"></select>
                                </div>
                            </div>
                            <div class="span3"></div>
                        </div>

                        <div class="control-group">
                            <div class="span4">
                                <label class="control-label" for="txtCashAmount">Cash Amount </label>
                                <div class="controls">
                                    <input type="number" id="txtCashAmount" class="span12">
                                </div>
                            </div>
                            <div class="span8">
                                <div class="controls">
                                    <label class="control-label" for="txtNotes">Notes </label>
                                    <div class="controls">
                                        <textarea id="txtNotes" class="span12 autosize" style="overflow: hidden; word-wrap: break-word; resize: both;"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                    <br />
                    @Html.Partial("_layoutPageButton")
                </form>
            </div>
        </div>
    </div>
    <div class="row-fluid">
        <div class="span12 widget">
            <div class="widget-header">
                <span class="title">
                    <i class="icol-table"></i> Store Cash
                </span>
            </div>
            <div id="divTable" class="widget-content table-container">
            </div>
        </div>
    </div>
</div>
@section scripts
{
    <script type="text/javascript">
        var base = '@Url.Content("~/")';
        $.getScript('@Url.Content("~/App_Js/Admin/StoreCash.js")',
            function (data, textStatus, jqxhr) {
                console.log('Script File Load Status=' + jqxhr.status);
            });
    </script>
}




